<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>SessionManag</servlet-name>
		<servlet-class>com.session.SessionLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SessionManag</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GoogleResp</servlet-name>
		<servlet-class>com.session.GoogleResp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleResp</servlet-name>
		<url-pattern>/googleresp</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Post</servlet-name>
		<servlet-class>com.session.PostServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Post</servlet-name>
		<url-pattern>/post/*</url-pattern>
	</servlet-mapping>
	
	<filter>
  <filter-name>ObjectifyFilter</filter-name>
  <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
</filter>
<filter-mapping>
  <filter-name>ObjectifyFilter</filter-name>
  <url-pattern>/*</url-pattern>
</filter-mapping>
<listener>
  <listener-class>com.session.helper.OfyHelper</listener-class>
</listener>
	
	<servlet>
		<servlet-name>GoogleServlet</servlet-name>
		<servlet-class>com.session.GoogleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleServlet</servlet-name>
		<url-pattern>/google</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserInfoServlet</servlet-name>
		<servlet-class>com.session.UserInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserInfoServlet</servlet-name>
		<url-pattern>/userinfo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.session.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>dashboard</servlet-name>
		<servlet-class>com.session.Check</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dashboard</servlet-name>
		<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.session.SignUp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login</welcome-file>
	</welcome-file-list>
</web-app>